#!/bin/bash
#SBATCH --mem=50GB
#SBATCH --gpus=v100:1
#SBATCH --time=00:35:00
#SBATCH --account=wang-krishnan

module purge
module load CUDA/11.8.0

source ~/anaconda3/etc/profile.d/conda.sh
conda activate ~/anaconda3/envs/pytorch

cd /mnt/scratch/f0106094/face2age

loss=mse
epochs=5
lr=0.0003
batch_size=64

python -u train.py \
-loss "$loss" \
-epochs "$epochs" \
--save \
-batch_size "$batch_size" \
-lr "$lr" \
-outdir results/test \
-out_prefix loss-"$loss"__batch_size-"$batch_size"__lr-"$lr" 

